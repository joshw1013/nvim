{
  "Math mode": {
    "prefix": ["ma", "math"],
    "body": ["\\( $1 \\)"],
    "descripiton": "Enter math mode",
    "luasnip": {
      "priority": 2000
    }
  },
  "Display math mode": {
    "prefix": ["dm", "dmath"],
    "body": ["\\[ $1 \\]"],
    "descripiton": "Enter display math mode",
    "luasnip": {
      "priority": 2000
    }
  },
  "Make vector": {
    "prefix": ["ve", "vec"],
    "body": ["\\vec{$1}"],
    "descripiton": "Begin a vector",
    "luasnip": {
      "priority": 2000
    }
  },
  "Add Curly Brace": {
    "prefix": "cb",
    "body": ["\\{ $1 \\\\}"],
    "descripiton": "Enter curly braces",
    "luasnip": {
      "priority": 2000
    }
  },
  "Begin a proof": {
    "prefix": ["proof"],
    "body": ["\\begin{proof}", "\t$1", "\\end{proof}"],
    "descripiton": "Enter proof",
    "luasnip": {
      "priority": 2000
    }
  },
  "Begin Parenthesis ": {
    "prefix": "par",
    "body": ["\\left( $1 \\\\right)"],
    "descripiton": "Enter parenthesis",
    "luasnip": {
      "priority": 2000
    }
  },
  "Begin bmatrix ": {
    "prefix": ["bm", "bmat"],
    "body": ["\\begin{bmatrix}", "\t$1", "\\end{bmatrix}"],
    "descripiton": "Enter bmatrix",
    "luasnip": {
      "priority": 2000
    }
  },
  "Make new line": {
    "prefix": "nl",
    "body": ["\\\\\\[${1:10}pt]"],
    "descripiton": "Add a new line",
    "luasnip": {
      "priority": 2000
    }
  },
  "Make subsubsection": {
    "scope": "latex,tex",
    "prefix": ["ss", "ss2"],
    "body": ["\\subsubsection*{$1}"],
    "descripiton": "Make subsubsection",
    "luasnip": {
      "priority": 2000
    }
  },
  "Make subsection": {
    "prefix": ["sb", "sss"],
    "body": ["\\subsection*{$1}"],
    "descripiton": "Make subsection",
    "luasnip": {
      "priority": 2000
    }
  },
  "Make section": {
    "prefix": "se",
    "body": ["\\section*{$1}"],
    "descripiton": "Make section",
    "luasnip": {
      "priority": 2000
    }
  },
  "Make transpose": {
    "prefix": ["tr", "trans"],
    "body": ["^\\intercal"],
    "descripiton": "Make transpose",
    "luasnip": {
      "priority": 2000
    }
  },
  "Make frak": {
    "prefix": ["mf", "mfrak"],
    "body": ["\\mathfrak{$1}"],
    "descripiton": "Make frak",
    "luasnip": {
      "priority": 2000
    }
  },
  "Make power": {
    "prefix": ["po", "pow"],
    "body": ["^{$1}"],
    "descripiton": "Make power",
    "luasnip": {
      "priority": 2000
    }
  },
  "Make underscore": {
    "scope": "latex,tex",
    "prefix": ["un", "under"],
    "body": ["_{$1}"],
    "descripiton": "Make underscore",
    "luasnip": {
      "priority": 2000
    }
  },
  "Make square root": {
    "prefix": ["sq", "sqrt"],
    "body": ["\\sqrt{$1}"],
    "descripiton": "Make square root",
    "luasnip": {
      "priority": 2000
    }
  },
  "Make fraction": {
    "prefix": ["fr", "frac"],
    "body": ["\\frac{$1}{$2}"],
    "descripiton": "Make square root",
    "luasnip": {
      "priority": 2000
    }
  },
  "Make Perp": {
    "prefix": ["pe", "perp"],
    "body": ["^\\perp"],
    "descripiton": "Make transpose",
    "luasnip": {
      "priority": 2000
    }
  },
  "Begin Arrows": {
    "prefix": ["an", "angle"],
    "body": ["\\langle $1 \\\\rangle"],
    "descripiton": "Enter parenthesis",
    "luasnip": {
      "priority": 2000
    }
  },
  "Begin Magnitude": {
    "prefix": ["mg", "mag"],
    "body": ["||$1||"],
    "descripiton": "Enter parenthesis"
  }
}
